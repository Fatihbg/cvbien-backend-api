{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python main_firebase.py",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "forceDeploy": true,
  "testPersistence": "2025-01-06-00:20",
  "forceRedeploy": "2025-01-06-00:25",
  "emergencyRedeploy": "2025-01-06-00:30",
  "useFirebase": true,
  "firebaseDeploy": "2025-01-06-00:35"
}